{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": ["./src/*"]
    }
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    ".next/types/**/*.ts",
    "app/infografis/page.js",
    "src/app/data-infografis/page.js",
    "src/components/ui/carousel.tsxx",
    "src/app/data-infografis/infografis/ipm/page.tsxx",
    "src/app/data-infografis/infografis/TabInfografis.tsx",
    "src/app/data-infografis/infografis/pertumbuhan-ekonomi/page.tsx",
    "src/app/data-infografis/infografis/ipm/page.tsxxx"
  ],
  "exclude": ["node_modules"]
}
